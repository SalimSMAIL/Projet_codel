<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.codel.entities.ContactGroup" table="CONTACT_GROUP">

		<id name="groupId" type="long" column="group_id">
			<generator class="increment" />
		</id>

		<property name="groupName" column="group_name" type="string" />
		<set name="contacts" table="CONTACT" cascade="all">
			<key>
				<column name="group_id" not-null="true" />
			</key>
			<many-to-many entity-name="com.codel.entities.Contact">
				<column name="contact_id" not-null="true" />
			</many-to-many>
		</set>
	</class>

</hibernate-mapping>